{
  "name": "astro-theme-starread",
  "type": "module",
    "version": "1.6.2",
    "files": ["src", "scripts", "public", "astro.config.mjs", "tsconfig.json"],
    "description": "既有星光般的璀璨，又不失简洁和高端感的Astro主题",
    "keywords": [
      "astro",
      "astro-theme",
      "blog-theme",
      "astro-blog"
    ],
  "author": "passwordgloo",
  "license": "CC-BY-SA-4.0",
  "homepage": "https://github.com/passwordgloo/astro-theme-starread#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/passwordgloo/astro-theme-starread.git"
  },
  "bugs": {
    "url": "https://github.com/passwordgloo/astro-theme-starread/issues"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npx pagefind --site dist --output-subdir _pagefind",
    "preview": "astro preview",
    "astro": "astro",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
